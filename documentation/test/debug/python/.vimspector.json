{
  "configurations": {
    "<name>: Launch": {
      "adapter": "debugpy",
      "configuration": {
        "name": "main: Launch",
        "type": "python",
        "request": "launch",
        "cwd": "~/.config/nvim/test/debug/python",
        "python": "/usr/bin/python",
        "stopOnEntry": true,
        "console": "externalTerminal",
        "debugOptions": [],
        "program": "~/.config/nvim/test/debug/python/main.py"
      }
    }
  }
}
